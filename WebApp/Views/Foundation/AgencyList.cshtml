@{
    ViewBag.Title = "AgencyList";
}
@model AR2AP.WebApp.Models.Foundation.SimpleListVModel<AR2AP.BLL.AgencyEntity>
@section submenu
{
    <div style=" font-size: 1.5em;
    text-align: center;
    padding: 0.5em 2em 0.5em 1em;
	background-color:#e2e2e2; margin-bottom:10px; font-weight:bold" class="ui-state-highlight ui-corner-all">
        Foundation
    </div>
    <ul id="leftmenu">
        <li><a href="#">Employee</a></li>
        <li><a href="#">Agency</a></li>
        <li><a href="#">Client</a></li>
        <li><a href="#">Team</a></li>
    </ul>
}
<div class="buttonset" style="margin:5px 0px; font-size:10px; font-weight:normal"><a>Add New Agency</a></div>
<table>
    <tr>
        <th>ID</th>
        <th>Agency Name</th>
        <th>Currency Type</th>
    </tr>

    @foreach (var item in Model.Entities)
    { 
        <tr>
            <td>@item.AgencyID</td>
            <td><a href="/Foundation/TermMerge/@item.AgencyID">@item.AgencyName</a></td>
            <td>@item.CurrencyType</td>
        </tr>
    }
</table>
<script>
    $(function () {
        $("#leftmenu").menu();
        $(".buttonset").buttonset();
    });
</script>
